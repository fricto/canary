        </menu>
        <menu name="Usage" inherit="bottom">
            <item name="Monitors" href="adding-a-monitor.html" />
            <item name="Response Handlers" href="adding-a-response-handler.html" />
            <item name="Notification Agents" href="adding-a-notification-agent.html" />
            <item name="Persistence Services" href="adding-a-persistence-service.html" />
            <item name="Bundle/Component State Monitoring" href="howto-bundle-component-state-monitoring.html" />
